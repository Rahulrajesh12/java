package com.arrays;
import java.util.Random;

public class copya2b 
{	
   public static void main(String[]args) 
 {
	   	Random r=new Random();
	   	int a[]=new int[] {1,2,4,4,5,4,4};
	   	int b[]=new int[a.length];
	   	for(int i=0;i<a.length;i++) 
        {	 
	         b[i]=a[i];
			 System.out.print(b[i]);

        }
	   }
	  }
	 
	 
 
	


