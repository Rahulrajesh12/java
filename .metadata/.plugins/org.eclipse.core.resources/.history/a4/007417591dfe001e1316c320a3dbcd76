package com.oops;

public class thisandsupermethod {

	public static void main(String[] args) 
	{
       m2 obj= new m2();
       obj.m2("raj");
       obj.m1("ram");
       
       
	}

}
class m1
{
	
 String name;
	
	void m1(String name)
	{
		this.name = name;
		System.out.println(name);
	}

}
class m2 extends m1
{
	void m2(String name)
	{
       super.name = name;
     System.out.println(name);
	}
}


   
       
     
	