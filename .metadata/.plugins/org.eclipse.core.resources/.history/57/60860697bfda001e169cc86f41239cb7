package src.com.pattens;

public class numpatterns {

	public static void main(String[] args) 
	{
      int rows=5;
      for(int r=1;r<=rows;r++)
      {
    	for(int c=rows;c>=r;c--) 
    	{
    	System.out.print("*");	
    	}
    	for(int c=rows;c=r;c++;)
    	System.out.println();
      }
	}

}
