package com.arrays;

public class foreachlengthproperty 
{

	public static void main(String[] args) 
	{
      int[]a=new int[]{1,2,3,4,5,6,7,8};
      for(int x=0;x<a.length/2;x++)
      {
         for(int x1=a.length-1/2;x>=0;x--)
         {
         
    	  System.out.print(a[x]);
         }
      
      
    	  System.out.print(a[x]);
      }
	}
}

    


