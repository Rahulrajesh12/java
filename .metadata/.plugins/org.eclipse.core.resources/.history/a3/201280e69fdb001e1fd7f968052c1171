package com.arrays;
import java.util.Random;
public class indexevenodd 
{
	  public static void main(String[]args)	
	{
	   Random r=new Random();
	   int sum=0,product=1;
	   int a[]=new int[5];
	   for(int i=0;i<a.length;i++)
	   {
		  a[i]= r.nextInt(1,9);
		  System.out.println(i);
		   if(i%2==0)
		   sum=sum+i;
	   }
	   System.out.println(sum);

	   
	}
}
