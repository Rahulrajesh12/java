package com.oops;

public class evilsclass {

	public static void main(String[] args)
	{
				       {
		         int num=9,copy=num,sum=0;
		          for(;num>0;num/=10)
		          {
		           num=num*num;
		           sum= num%10;
		           sum=sum+sum;
		          }
		           if(copy==sum)
		           System.out.println("Evil Number");
		           else 
		           System.out.println("Not Evil Number");
		      }
		}
	}


